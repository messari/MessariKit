{
	"name": "messari-kit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "pnpm -r build",
		"test": "pnpm -r test:run",
		"test:watch": "pnpm -r test:watch",
		"clean": "pnpm -r clean && rimraf typegen/openapi/dist",
		"lint:fix": "pnpm biome lint --write . && pnpm biome format --write .",
		"examples:start": "pnpm --filter @messari/sdk-examples start:all",
		"examples:start:ai": "pnpm --filter @messari/sdk-examples start:ai",
		"examples:start:asset": "pnpm --filter @messari/sdk-examples start:asset",
		"examples:start:asset:ath": "pnpm --filter @messari/sdk-examples start:asset:ath",
		"examples:start:asset:roi": "pnpm --filter @messari/sdk-examples start:asset:roi",
		"examples:start:asset:timeseries": "pnpm --filter @messari/sdk-examples start:asset:timeseries",
		"examples:start:exchanges": "pnpm --filter @messari/sdk-examples start:exchanges",
		"examples:start:markets": "pnpm --filter @messari/sdk-examples start:markets",
		"examples:start:networks": "pnpm --filter @messari/sdk-examples start:networks",
		"examples:start:diligence": "pnpm --filter @messari/sdk-examples start:diligence",
		"examples:start:fundraising": "pnpm --filter @messari/sdk-examples start:fundraising",
		"examples:start:intel": "pnpm --filter @messari/sdk-examples start:intel",
		"examples:start:news": "pnpm --filter @messari/sdk-examples start:news",
		"examples:start:research": "pnpm --filter @messari/sdk-examples start:research",
		"examples:start:token-unlocks": "pnpm --filter @messari/sdk-examples start:token-unlocks",
		"examples:start:user-management": "pnpm --filter @messari/sdk-examples start:user-management",
		"api:validate": "redocly lint typegen/openapi/**/*.yaml",
		"api:bundle": "redocly bundle typegen/openapi/index2.yaml --output typegen/openapi/dist/combined.yaml",
		"api:docs": "redocly preview-docs typegen/openapi/index2.yaml",
		"api:types": "./typegen/scripts/generate-types.sh && tsx typegen/scripts/generate-operation-types.ts",
		"api:build": "pnpm run clean && pnpm run api:validate && pnpm run api:bundle && pnpm run api:types && pnpm run build"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@redocly/cli": "^1.31.2",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^20.11.24",
		"js-yaml": "^4.1.0",
		"openapi-types": "^12.1.3",
		"openapi-typescript": "^6.7.4",
		"rimraf": "^5.0.5",
		"tsx": "^4.7.1",
		"typescript": "^5.3.3",
		"vitest": "^3.0.7"
	}
}